{
  "name": "medical-research-tracker",
  "version": "0.1.63",
  "private": true,
  "description": "Personal health records and medical research discovery tool",
  "author": "John Perkins <john@mytreatmentpath.com>",
  "main": "electron/main.cjs",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "server": "node server/index.js",
    "start": "npm run server & npm run dev",
    "electron:dev": "concurrently -k \"cross-env NODE_ENV=development npm run dev\" \"wait-on http://localhost:5173 && cross-env NODE_ENV=development electron .\"",
    "electron:build": "npm run build && node build-server.js && electron-builder",
    "electron:build:mac": "npm run build && node build-server.js && electron-builder --mac",
    "electron:build:win": "npm run build && electron-builder --win",
    "electron:build:linux": "npm run build && electron-builder --linux",
    "electron:rebuild": "echo 'Native modules will be rebuilt during electron:build (npmRebuild: true)'",
    "postinstall": "npx @electron/rebuild -f -m ./node_modules/better-sqlite3",
    "notarize": "./build-notarized.sh",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:ipc": "playwright test --project=ipc-modules",
    "test:ipc-coverage": "playwright test --project=ipc-coverage",
    "test:api": "playwright test --project=api-tests"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.8",
    "@mui/material": "^7.3.8",
    "@supabase/supabase-js": "^2.95.3",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.6.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cytoscape": "^3.33.1",
    "dotenv": "^17.3.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.5.2",
    "jsonwebtoken": "^9.0.3",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "openai": "^6.21.0",
    "pdf-parse": "^1.1.1",
    "playwright": "^1.58.2",
    "react-cytoscapejs": "^2.0.0",
    "reactflow": "^11.11.4",
    "recharts": "^3.7.0"
  },
  "devDependencies": {
    "@electron/notarize": "^3.1.1",
    "@electron/rebuild": "^4.0.3",
    "@playwright/test": "^1.58.2",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^40.4.1",
    "electron-builder": "^26.7.0",
    "electron-rebuild": "^3.2.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^5.0.8",
    "wait-on": "^9.0.4"
  }
}
