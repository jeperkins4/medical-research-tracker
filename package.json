{
  "name": "medical-research-tracker",
  "version": "0.1.0",
  "private": true,
  "description": "Personal health records and medical research discovery tool",
  "author": "John Perkins <john@mytreatmentpath.com>",
  "main": "electron/main.cjs",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "server": "node server/index.js",
    "start": "npm run server & npm run dev",
    "electron:dev": "concurrently -k \"cross-env NODE_ENV=development npm run dev\" \"wait-on http://localhost:5173 && cross-env NODE_ENV=development electron .\"",
    "electron:build": "npm run build && electron-builder",
    "electron:build:mac": "npm run build && electron-builder --mac",
    "electron:build:win": "npm run build && electron-builder --win",
    "electron:build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.8",
    "@mui/material": "^7.3.8",
    "@supabase/supabase-js": "^2.95.3",
    "bcryptjs": "^3.0.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cytoscape": "^3.33.1",
    "dotenv": "^17.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.3",
    "node-cron": "^4.2.1",
    "openai": "^6.21.0",
    "playwright": "^1.58.2",
    "react-cytoscapejs": "^2.0.0",
    "reactflow": "^11.11.4",
    "recharts": "^3.7.0"
  },
  "optionalDependencies": {
    "better-sqlite3": "^12.6.2",
    "better-sqlite3-multiple-ciphers": "^11.10.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^40.4.1",
    "electron-builder": "^26.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^5.0.8",
    "wait-on": "^9.0.4"
  }
}
